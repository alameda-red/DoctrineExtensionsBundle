<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:doctrine="http://symfony.com/schema/dic/doctrine"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd
        http://symfony.com/schema/dic/doctrine
        http://symfony.com/schema/dic/doctrine/doctrine-1.0.xsd">

    <doctrine:config>
        <doctrine:dbal>
            <doctrine:type name="dateinterval">Alameda\Bundle\DoctrineExtensionsBundle\Types\DateIntervalType</doctrine:type>
            <doctrine:type name="timestamp">Alameda\Bundle\DoctrineExtensionsBundle\Types\TimestampType</doctrine:type>
        </doctrine:dbal>
    </doctrine:config>
</container>